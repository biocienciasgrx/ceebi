{
	"appId": "es.biociencias.ceebi",
	"appName": "CEEBI",
	"webDir": "dist",
	"bundledWebRuntime": true,
	"loggingBehavior": "debug",
	"plugins": {
		"LocalNotifications": {
			"smallIcon": "ic_launcher_round",
			"iconColor": "#34b6ed"
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"SplashScreen": {
			"launchAutoHide": false,
			"androidScaleType": "FIT_CENTER",
			"backgroundColor": "#ffffff",
			"androidSplashResourceName": "splash"
		}
	}
}
